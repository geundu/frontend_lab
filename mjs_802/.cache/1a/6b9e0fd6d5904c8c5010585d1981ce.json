erated":{"line":57,"column":38},"source":"app7.js","original":{"line":125,"column":38}},{"generated":{"line":57,"column":44},"source":"app7.js","original":{"line":125,"column":0}},{"name":"router","generated":{"line":58,"column":0},"source":"app7.js","original":{"line":126,"column":0}},{"generated":{"line":58,"column":6},"source":"app7.js","original":{"line":126,"column":6}}],"sources":{"app7.js":"const ajax = new XMLHttpRequest();\r\nconst container = document.getElementById('root');\r\nconst content = document.createElement('div');\r\nconst NEWS_URL = 'https://api.hnpwa.com/v0/news/1.json';\r\nconst CONTENT_URL = 'https://api.hnpwa.com/v0/item/@id.json';\r\n// 페이지 처리 관련 추가\r\n// 상태 관리한다. -> 라이프사이클 -> heap, stack, queue\r\nconst store = {\r\n  currentPage: 1,\r\n};\r\n\r\nfunction getData(url) {\r\n  ajax.open('GET', url, false);\r\n  ajax.send();\r\n  return JSON.parse(ajax.response);\r\n} // end of getData()\r\n\r\nfunction newsFeed() {\r\n  const newsFeed = getData(NEWS_URL);\r\n  const newsList = [];\r\n\r\n  let template = `\r\n  <div class=\"bg-gray-600 min-h-screen\">\r\n    <div class=\"bg-white text-xl\">\r\n      <div class=\"mx-auto px-4\">\r\n        <div class=\"flex justify-between items-center py-6\">\r\n          <div class=\"flex justify-start\">\r\n            <h1 class=\"font-extrabold\">Hacker News</h1>\r\n          </div>\r\n          <div class=\"items-center justify-end\">\r\n            <a href=\"#/page/{{__prev_page__}}\" class=\"text-gray-500\">\r\n              Previous\r\n            </a>\r\n            <a href=\"#/page/{{__next_page__}}\" class=\"text-gray-500 ml-4\">\r\n              Next\r\n            </a>\r\n          </div>\r\n        </div> \r\n      </div>\r\n    </div>\r\n    <div class=\"p-4 text-2xl text-gray-700\">\r\n        {{__news_feed__}}\r\n    </div>\r\n  </div>\r\n  `;\r\n\r\n  for (let i = (store.currentPage - 1) * 10; i < store.currentPage * 10; i++) {\r\n    newsList.push(\r\n      `\r\n      <div class=\"p-6 bg-white mt-6 rounded-lg shadow-md transition-colors duration-500 hover:bg-green-100\">\r\n        <div class=\"flex\">\r\n          <div class=\"flex-auto\">\r\n            <a href=\"#/show/${newsFeed[i].id}\">${newsFeed[i].title}</a>  \r\n          </div>\r\n          <div class=\"text-center text-sm\">\r\n            <div class=\"w-10 text-white bg-green-300 rounded-lg px-0 py-2\">${newsFeed[i].comments_count}</div>\r\n          </div>\r\n        </div>\r\n        <div class=\"flex mt-3\">\r\n          <div class=\"grid grid-cols-3 text-sm text-gray-500\">\r\n            <div><i class=\"fas fa-user mr-1\"></i>${newsFeed[i].user}</div>\r\n            <div><i class=\"fas fa-heart mr-1\"></i>${newsFeed[i].points}</div>\r\n            <div><i class=\"far fa-clock mr-1\"></i>${newsFeed[i].time_ago}</div>\r\n          </div>  \r\n        </div>\r\n      </div>\r\n      `\r\n    );\r\n  }\r\n\r\n  template = template.replace('{{__news_feed__}}', newsList.join(''));\r\n  template = template.replace('{{__prev_page__}}', store.currentPage > 1 ? store.currentPage - 1 : 1);\r\n  template = template.replace('{{__next_page__}}', store.currentPage + 1);\r\n\r\n  container.innerHTML = template;\r\n} // end of newsFeed()\r\n\r\nfunction newsDetail() {\r\n  const id = location.hash.substr(7);\r\n  const newsContent = getData(CONTENT_URL.replace('@id', id));\r\n\r\n  let template = `\r\n  <div class=\"bg-gray-600 min-h-screen pb-8\">\r\n    <div class=\"bg-white text-xl\">\r\n      <div class=\"mx-auto px-4\">\r\n        <div class=\"flex justify-between items-center py-6\">\r\n          <div class=\"flex justify-start\">\r\n            <h1 class=\"font-extrabold\">Hacker News</h1>\r\n          </div>\r\n          <div class=\"items-center justify-end\">\r\n            <a href=\"#/page/${store.currentPage}\" class=\"text-gray-500\">\r\n              <i class=\"fa fa-times\"></i>\r\n            </a>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  \r\n    <div class=\"h-full border rounded-xl bg-white m-6 p-4 \">\r\n      <h2>${newsContent.title}</h2>\r\n      <div class=\"text-gray-400 h-20\">\r\n        ${newsContent.content}\r\n      </div>\r\n  \r\n      {{__comments__}}\r\n  \r\n    </div>\r\n  </div>   \r\n  `;\r\n  container.innerHTML = template;\r\n} // end of newsDetail()\r\n\r\nfunction router() {\r\n  const routePath = location.hash;\r\n  if (routePath === '') {\r\n    newsFeed();\r\n  } else if (routePath.indexOf('#/page/') >= 0) {\r\n    store.currentPage = Number(routePath.substr(7));\r\n    newsFeed();\r\n  } else {\r\n    newsDetail();\r\n  }\r\n} // end of router()\r\n\r\nwindow.addEventListener('hashchange', router);\r\nrouter();\r\n"},"lineCount":null}},"error":null,"hash":"873204221e282a7deaab2caa18ef4f52","cacheData":{"env":{}}}